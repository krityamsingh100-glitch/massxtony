{
    "name": "Telegram Report Manager",
    "description": "⚠️ WARNING: This bot is for educational purposes only. Misuse may violate Telegram's Terms of Service and lead to account suspension.",
    "warning": "Use responsibly and only on content that truly violates Telegram's rules.",
    "logo": "https://graph.org/file/6c7ec3b6c2b2bfaf85203.jpg",
    "keywords": ["Telegram", "Bot", "Management"],
    "repository": "https://github.com/StarkFRG/Telegram-Report-Bot",
    "env": {
        "API_ID": {
            "description": "Your APP ID from my.telegram.org",
            "value": "",
            "required": true
        },
        "API_HASH": {
            "description": "Your API Hash from my.telegram.org",
            "value": "",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "Your Bot Token from @BotFather",
            "value": "",
            "required": true
        },
        "OWNER_ID": {
            "description": "Your Telegram User ID",
            "value": "",
            "required": true
        },
        "REPORT_COOLDOWN": {
            "description": "Delay between reports in seconds (recommended: 5)",
            "value": "5",
            "required": false
        },
        "MAX_ACCOUNTS": {
            "description": "Maximum number of accounts allowed (recommended: 10)",
            "value": "10",
            "required": false
        }
    },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
